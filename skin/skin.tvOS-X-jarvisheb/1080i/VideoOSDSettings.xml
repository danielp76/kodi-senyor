<?xml version="1.0" encoding="UTF-8"?>
<window id="123">
  <defaultcontrol always="true">5</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <controls>
    <control type="group">
      <left>1020</left>
      <top>0</top>
      <animation effect="slide" tween="circle" start="1050,0" end="0,0" time="400" easing="Out">windowopen</animation>
      <animation effect="slide" tween="back" start="0,0" end="1050,0" time="400" easing="In">windowclose</animation>
      <animation effect="fade" start="100" end="0" time="300" condition="Window.IsVisible(SliderDialog) | Window.IsVisible(FileBrowser)">Conditional</animation>
      <control type="image" description="background image">
        <left>-50</left>
        <width>1050</width>
        <height>1080</height>
		<include>seekbar_bkgd</include>
        <aspectratio>stretch</aspectratio>
      </control>
      <control type="label" id="2" description="Title">
        <left>45</left>
        <width>780</width>
        <font>size33B</font>
        <align>center</align>
		<textcolor>textcolor2</textcolor>
      </control>
      <control type="grouplist" id="5" description="Settings">
        <left>15</left>
        <top>90</top>
        <width>840</width>
        <height>940</height>
        <itemgap>20</itemgap>
        <orientation>vertical</orientation>
        <pagecontrol>60</pagecontrol>
        <onup>5</onup>
        <ondown>5</ondown>
      </control>
      <control type="button" id="7">
        <description>Default Button</description>
        <!--texturenofocus>-</texturenofocus-->
        <font>size30</font>
        <align>left</align>
        <aligny>center</aligny>
        <height>60</height>
      </control>
      <control type="radiobutton" id="8">
        <description>Default RadioButton</description>
        <!--texturenofocus>-</texturenofocus-->
        <align>left</align>
        <font>size30</font>
        <height>60</height>
        <aligny>center</aligny>
      </control>
      <control type="spincontrolex" id="9">
        <description>Default spincontrolex</description>
        <!--texturenofocus>-</texturenofocus-->
        <align>left</align>
        <font>size30</font>
        <height>60</height>
        <aligny>center</aligny>
        <reverse>yes</reverse>
      </control>
      <control type="sliderex" id="13">
        <description>Default Slider</description>
        <!--texturenofocus>-</texturenofocus-->
        <align>left</align>
        <font>size30</font>
        <aligny>center</aligny>
        <height>60</height>
      </control>
    </control>
  </controls>
</window>
